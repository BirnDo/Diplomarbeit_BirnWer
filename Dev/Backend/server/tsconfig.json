{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitAny": true,
    "esModuleInterop": true,
    "outDir": "build",
    "allowJs": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "resolveJsonModule": true
  },
  "include": ["*.ts", "routes/*.ts", "environment/env.js", "swagger.json"],
  "exclude": ["node_modules"]
}
